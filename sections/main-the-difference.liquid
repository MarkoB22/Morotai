<script>
  $(document).ready(function() {
    var swipeElement160 = $('.Frame160 .Frame146');
    setupSwipeDetection(swipeElement160, 'Frame160');

    var swipeElement168 = $('.Frame168 .Frame146');
    setupSwipeDetection(swipeElement168, 'Frame168');

    function setupSwipeDetection(swipeElement, parentFrame) {
      var startX, startY;
      var minSwipeDistance = 50;

      swipeElement.on('touchstart', function(e) {
        startX = e.originalEvent.touches[0].pageX;
        startY = e.originalEvent.touches[0].pageY;
      });

      swipeElement.on('touchend', function(e) {
        var endX = e.originalEvent.changedTouches[0].pageX;
        var endY = e.originalEvent.changedTouches[0].pageY;

        var deltaX = endX - startX;
        var deltaY = endY - startY;

        if (Math.abs(deltaX) > minSwipeDistance && Math.abs(deltaX) > Math.abs(deltaY)) {
          if (deltaX > 0) {
            handleSwipe('right', parentFrame);
          } else {
            handleSwipe('left', parentFrame);
          }
        }
      });
    }

    var testval = '{{language_param }}';

    function handleSwipe(direction) {
      if(direction == 'right') {
        if(testval == 'de') {
          updateTextEnglish('right', 'de')
          updateTextEnglish2('right', 'de')
        }
        else if(testval == 'en') {
          updateTextEnglish('right', 'en')
          updateTextEnglish2('right', 'en')
        }
      }
       else if(direction == 'left') {
                 if(testval == 'de') {
                   updateTextEnglish('left', 'de')
                   updateTextEnglish2('left', 'de')
                 }
        else if(testval == 'en') {
          updateTextEnglish('left', 'en')
          updateTextEnglish2('left', 'en')}
      }
    }
  });
</script>

