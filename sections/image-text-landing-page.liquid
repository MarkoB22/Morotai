{{- 'image-text-landing-page.css' | asset_url | stylesheet_tag -}}
<div class="image_text_landing-wrapper">
  <div class="image_text_landing-image">
    {%- liquid
       assign image = section.settings.image
        assign master_width = image.src.width | append: 'x' | append: image.src.height
        assign width = '375x' | append: mobile_image_height | append: ',' | append: master_width
        assign class = nil
     -%}
    {%- render 'responsive-image', image: image, sizes: width, class: class -%}
  </div>
  <div class="image_text_landing-text">
  
  </div>
</div>
{% schema %}
  {
    "name": "Image text landing",
    "settings": [
      
    ]
  }
{% endschema %}